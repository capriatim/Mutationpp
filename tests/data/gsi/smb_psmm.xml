<gsi gsi_mechanism = "detailed_mass">

    <surface_properties type = "detailed" steady_coverage = "true">
        <site label = "s" sites_per_m2 = "1.e20" species = "O" />
        <surface label="b" species="C" />
    </surface_properties>

    <surface_chemistry>
        <!-- Adsorption of O Atoms -->
        <reaction type="detailed" formula="O + s => O-s">
            <adsorption stick_coef = "1."  T = "0." />
        </reaction>

        <!-- Thermal Desorption of O Atoms -->
        <reaction type="detailed" formula="O-s => O + s">
            <desorption_tst T = "4.427745"/>
        </reaction>

        <!-- CO production -->
        <reaction type="detailed" formula="O + O-s + C-b => CO + O-s">
            <ER_arrhenius pre_exp = "57.37" T = "4.667e3"/>
        </reaction>

        <!-- CO production -->
        <reaction type="detailed" formula="O + O-s + C-b => CO2 + s">
            <ER_arrhenius  pre_exp = "8.529e-6" T = "-6.958e3"/>
        </reaction>

        <!-- CO2 production -->
        <reaction type="detailed" formula="O + s + C-b => CO + s">
            <ER_arrhenius  pre_exp = "0.1203" T = "-2.287e3"/>
        </reaction>
    </surface_chemistry>
</gsi>